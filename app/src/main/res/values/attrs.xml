<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InformView">
        <attr name="iv_message" format="string" />
        <attr name="iv_style" format="integer">
            <enum name="info" value="0" />
            <enum name="success" value="1" />
            <enum name="warning" value="2" />
            <enum name="error" value="3" />
        </attr>
        <attr name="iv_textAlignment" format="integer">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="iv_isIconHidden" format="boolean" />
        <attr name="iv_canDismiss" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StateView">
        <attr name="sv_iconSrc" format="reference" />
        <attr name="sv_title" format="string" />
        <attr name="sv_subtitle" format="string" />
        <attr name="sv_actionText" format="string" />
        <attr name="sv_backgroundColor" format="color" />
    </declare-styleable>
</resources>